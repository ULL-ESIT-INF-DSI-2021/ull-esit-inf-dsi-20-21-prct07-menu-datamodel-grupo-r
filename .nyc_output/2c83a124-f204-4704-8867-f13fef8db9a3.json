n\n  /** ******************************************************************** **/\n\n  /**\n   * Devuelve las proteínas.\n   */\n  public get proteins(): number {\n    return this.proteins_;\n  }\n\n  /**\n   * Cambia el valor de las proteínas.\n   */\n  public set proteins(newProteins: number) {\n    this.proteins_ = newProteins;\n  }\n\n  /** ******************************************************************** **/\n\n  /**\n   * Devuelve los lípidos.\n   */\n  public get lipids(): number {\n    return this.lipids_;\n  }\n\n  /**\n   * Cambia el valor de los lípidos.\n   */\n  public set lipids(newLipids: number) {\n    this.lipids_ = newLipids;\n  }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"1a4e4a59f35ea117b85d53c04a1f594a83867975","contentHash":"5b3ac0ab77af4cf8d0a69e850ef3678567d5b4fba575750a2f4e5791a8e9bfd9"},"/mnt/d/Home/Universidad/DSI/ull-esit-inf-dsi-20-21-prct07-menu-datamodel-grupo-r/src/Plate.ts":{"path":"/mnt/d/Home/Universidad/DSI/ull-esit-inf-dsi-20-21-prct07-menu-datamodel-grupo-r/src/Plate.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":43}},"2":{"start":{"line":4,"column":19},"end":{"line":4,"column":42}},"3":{"start":{"line":5,"column":23},"end":{"line":5,"column":50}},"4":{"start":{"line":10,"column":0},"end":{"line":15,"column":62}},"5":{"start":{"line":11,"column":4},"end":{"line":11,"column":38}},"6":{"start":{"line":12,"column":4},"end":{"line":12,"column":45}},"7":{"start":{"line":13,"column":4},"end":{"line":13,"column":47}},"8":{"start":{"line":14,"column":4},"end":{"line":14,"column":36}},"9":{"start":{"line":20,"column":27},"end":{"line":123,"column":3}},"10":{"start":{"line":27,"column":8},"end":{"line":27,"column":26}},"11":{"start":{"line":28,"column":8},"end":{"line":28,"column":40}},"12":{"start":{"line":29,"column":8},"end":{"line":29,"column":72}},"13":{"start":{"line":30,"column":8},"end":{"line":30,